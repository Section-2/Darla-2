@{
    var teamMembers = ViewBag.TeamMembers as List<User>;
}
<h1>This is the Group Peer Eval page that will show the list of all the group members other than the user and when the user clicks on that members name it will take them to that members StudentPeerReview</h1>




@if (teamMembers != null && teamMembers.Any())
{
    <h2>Team Members</h2>
    <ul>
        @foreach (var member in teamMembers)
        {
            <li>@member.FirstName @member.LastName</li>
        }
    </ul>
}
else
{
    <p>No team members found.</p>
}