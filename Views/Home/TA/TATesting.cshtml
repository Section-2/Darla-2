@model Darla.Models.IntexViewModel

<h1>Every Table in our Database</h1>

<h2>Grades</h2>
<table class="table">
    <thead>
        <tr>
            <th>AssignmentId</th>
            <th>UserId</th>
            <th>TeamNumber</th>
            <th>PointsEarned</th>
            <th>Comments</th>
            <th>GradeId</th>
            <!-- Add more columns as needed -->
        </tr>
    </thead>
    <tbody>
        @foreach (var grade in Model.Grades)
        {
            <tr>
                <td>@grade.AssignmentId</td>
                <td>@grade.UserId</td>
                <td>@grade.TeamNumber</td>
                <td>@grade.PointsEarned</td>
                <td>@grade.Comments</td>
                <td>@grade.GradeId</td>
                <!-- Add more columns as needed -->
            </tr>
        }
    </tbody>
</table>
