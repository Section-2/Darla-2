@{
}
@* @model ScaffoldModel<something>
 *@
<table class="table table-striped">
    <thead class="fw-bold">
        <tr>
            <td>User ID</td>
            <td>Net ID</td>
            <td>First Name</td>
            <td>Last Name</td>
            <td>Permission Type</td>
        </tr>
    </thead>

    @foreach (var x in Model)
    {
        <tbody>
            <tr>
                <td>@x.user_id</td>
                <td>@x.net_id</td>
                <td>@x.first_name</td>
                <td>@x.last_name</td>
                <td>@x.permission_type</td>

                <td><a class="btn btn-info" asp-controller="Home" asp-action="Edit" asp-route-id="@x.MovieId">Edit</a></td>
                <td><a class="btn btn-danger" asp-controller="Home" asp-action="Delete" asp-route-id="@x.MovieId">Delete</a></td>
            </tr>
        </tbody>

    }
</table>